INFLUXDB_URL=http://localhost:8086
INFLUXDB_TOKEN=your-token-here
INFLUXDB_ORG=monitorx
INFLUXDB_BUCKET=metrics
LOG_LEVEL=INFO
API_HOST=0.0.0.0
API_PORT=8000
DASHBOARD_PORT=8501

# Security Configuration
API_KEY_ENABLED=false
API_KEYS=key1,key2,key3
JWT_ENABLED=false
JWT_SECRET_KEY=change-this-in-production-use-long-random-string
JWT_EXPIRE_MINUTES=30

# Rate Limiting Configuration
RATE_LIMIT_ENABLED=false
RATE_LIMIT_REQUESTS=100
RATE_LIMIT_WINDOW=60

# CORS Configuration
CORS_ORIGINS=*
CORS_ALLOW_CREDENTIALS=true

# Email Alert Configuration
ENABLE_EMAIL_ALERTS=false
SMTP_SERVER=smtp.gmail.com
SMTP_PORT=587
SMTP_USERNAME=your-email@gmail.com
SMTP_PASSWORD=your-app-password
ALERT_FROM_EMAIL=alerts@yourcompany.com
ALERT_TO_EMAILS=team@yourcompany.com,oncall@yourcompany.com

# Slack Alert Configuration
ENABLE_SLACK_ALERTS=false
SLACK_WEBHOOK_URL=https://hooks.slack.com/services/YOUR/SLACK/WEBHOOK
SLACK_CHANNEL=#ml-alerts

# Webhook Alert Configuration
ENABLE_WEBHOOK_ALERTS=false
WEBHOOK_URL=https://api.yourcompany.com/alerts
WEBHOOK_API_KEY=your-api-key